<VBox alignment="CENTER" prefHeight="722.0" prefWidth="929.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="edu.farmingdale.csc311_gui_basics.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="58.0" prefWidth="130.0" />

    <Label fx:id="welcomeText" />
   <Separator orientation="VERTICAL" prefHeight="200.0" />
   <Label fx:id="name" rotate="-55.0" text="Jason Devaraj">
      <font>
         <Font size="32.0" />
      </font>
   </Label>
    <Button onAction="#onHelloButtonClick" text="Hello!" />
   <TextField />
   <AreaChart>
     <xAxis>
       <CategoryAxis side="BOTTOM" />
     </xAxis>
     <yAxis>
       <NumberAxis side="LEFT" />
     </yAxis>
   </AreaChart>
   <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
         <Circle fill="DODGERBLUE" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
         <Slider orientation="VERTICAL" />
         <Polygon fill="DODGERBLUE" stroke="BLACK" strokeType="INSIDE">
           <points>
             <Double fx:value="-50.0" />
             <Double fx:value="40.0" />
             <Double fx:value="50.0" />
             <Double fx:value="40.0" />
             <Double fx:value="0.0" />
             <Double fx:value="-60.0" />
           </points>
         </Polygon>
         <Ellipse fill="DODGERBLUE" radiusX="100.0" radiusY="80.0" stroke="BLACK" strokeType="INSIDE" />
         <Cylinder height="100" radius="20" />
         <ProgressIndicator prefHeight="91.0" prefWidth="113.0" progress="0.0" />
         <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@pexels-pixabay-68603.jpg" />
            </image>
         </ImageView>
         <CubicCurve controlX1="-50.0" controlX2="50.0" controlY1="-100.0" controlY2="100.0" endX="150.0" fill="DODGERBLUE" startX="-150.0" stroke="BLACK" />
      </children>
   </HBox>
</VBox>
